{
    "opts": {
        "destination": "./out/",
        "encoding": "utf8",
        "recurse": true,
        "template": "node_modules/minami",
        "readme": "README.md"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["server/", "src", "node_modules/@inexorgame/tree"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "node_modules"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
